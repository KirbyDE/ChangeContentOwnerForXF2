    "enabled": true,
    "action": "str_replace",
    "find": "<xf:if is=\"$hasActionBarMenu\">",
    "replace": "<xf:macro template=\"tckChangeContentOwner_macros\" name=\"action_link\"\n\t\t  arg-content=\"{$album}\"\n\t\t  arg-route=\"media/albums\" />\n\n$0"
}