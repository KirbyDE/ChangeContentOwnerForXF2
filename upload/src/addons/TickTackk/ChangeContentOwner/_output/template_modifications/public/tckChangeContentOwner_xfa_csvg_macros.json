{
    "template": "xfa_csvg_macros",
    "description": "Add \"Change owner or date\" in graph tools menu",
    "execution_order": 10,
    "enabled": true,
    "action": "preg_replace",
    "find": "#(<xf:if is=\"\\$graph.canDelete\\(\\).*\">.*?<\\/xf:if>)(.*?)(<\\/xf:contentcheck>)#si",
    "replace": "$1\n\n<xf:macro template=\"tckChangeContentOwner_macros\" name=\"menu_link_row\" arg-content=\"{$graph}\" arg-route=\"graphs\" />\n\n$2$3"
}